{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "IspSession": "localhost:6379"
  },
  "IspSession": {
    // a session cookie can be 'abandoned' but correlation remains
    "CorrellationCookieName": "IspSessionCorrelation",
    "SessionCookieName": "Session",
    // we support Cookie, IPAddress and CustomHeader
    "AffinityMethod": "Cookie",
    "ApplicationName": "NCV_demo",
    // redis keys will be encrypted using synchrone encryption
    // somebody with access to Redis cannot correlate session cookies with anything written in Redis
    "KeyEncryptionSecret": "secret",
    // give your own secret which should be 32 character aligned hexadecimal
    "KeyIVSecret": "01234567812345ab01234567812345ab",
    "License": "6F9ADD28D92B6F1EF7EB7A7A0FF6426F0799500E32289019BB92725DBC598BE6",
    "CompressData": true
  }
}