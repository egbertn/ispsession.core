    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (web - Debug)",
            "type": "coreclr",
            "request": "launch",
            "console": "integratedTerminal",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/net8.0/demo.dll",
            "args": [],
            "cwd": "${workspaceFolder}/",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ASPNETCORE_HOSTBUILDER__RELOADCONFIGONCHANGE": "false",
                "ASPNETCORE_URLS": "https://0.0.0.0:7058;http://0.0.0.0:5045"
            }
        },
    ]
}